{
  "name": "billd-scss",
  "version": "0.0.3",
  "description": "billd's scss collections",
  "keywords": [
    "billd",
    "scss",
    "sass"
  ],
  "homepage": "https://github.com/galaxy-s10/billd-scss",
  "bugs": {
    "url": "https://github.com/galaxy-s10/billd-scss/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/galaxy-s10/billd-scss"
  },
  "license": "MIT",
  "author": "shuisheng <https://github.com/galaxy-s10>",
  "files": [
    "src",
    "./postinstall.js"
  ],
  "scripts": {
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0",
    "cz": "cz",
    "postinstall": "node ./postinstall.js",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "release:local": "node ./scripts/release.js",
    "release:online": "node ./scripts/publish.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "chalk": "^4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.0",
    "inquirer": "^8",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "semver": "^7.3.7"
  }
}
